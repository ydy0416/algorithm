    "# N(1<=N<=500,000)\n",
    "from collections import deque\n",
    "n=int(input())\n",
    "q=deque(i for i in range(1,n+1))\n",
    "while len(q)>1:\n",
    "    q.popleft()\n",
    "    q.append(q.popleft())\n",
    "print(q[0])"
   ]
  }
